	function LoadNotifition(date) {
		checkLoadNotification = true;
    $.ajax({
        url: "/Notification/LoadNotification",
        type: "GET",
		data: {date:date,quantity:10},
        dataType: "json",
        success: function (result) {
			if (result.result == @Constant.SUCCESS) {
				if (result.data.length == 0) {
					checkLoadNotification = true;
					var htmlLoad = '<li class="row  user-notification">'+
									'<div class="user-request-list ">' +
										'<div class="accept-btns user-follow-button">' +
											"You don't have any new Notification"+
										'</div>' +
									'</div>' +
								'</li>';
					$("#notifications-list").append(htmlLoad)
					return;
				}

				if (result.data.length != 0 && date == undefined) {
					$("#notifications-list").empty();
				}
				dateGetNotification = parseInt(result.data[result.data.length - 1].CreatedTime.substr(6)) ;
				var countnotification = 0;
				if (date != undefined) {
					countnotification = $("#alert-notification").html();
				}
				result.data.forEach((request) => {
					var active = "active";
					if (request.Seen) {
						active = "un-active";
					}
					else {
						countnotification ++;
					}
					var html = '<li class="row '+active +'  user-notification-'+ request.ID +'" onclick="SeenNotification('+ request.ID +')">'+
									'<div class="user-request-list ">' +
										'<div class="accept-btns user-follow-button">' +
											request.Content
										'</div>' +
									'</div>' +
								'</li>';
					$("#notifications-list").append(html)
				});
				if (countnotification == 0) {
					$("#alert-notification").html("");
				}
				else {
					$("#alert-notification").html(countnotification);
				}
				if (result.data.length < 10) {
					checkLoadNotification = true;
					var htmlLoad = '<li class="row  user-notification">' +
						'<div class="user-request-list ">' +
						'<div class="accept-btns user-follow-button">' +
						"You don't have any new Notification" +
						'</div>' +
						'</div>' +
						'</li>';
					$("#notifications-list").append(htmlLoad)
				}
				else {
					checkLoadNotification = false;
				}
			}
        },
        error: function (data) {
        }
    });
	}
